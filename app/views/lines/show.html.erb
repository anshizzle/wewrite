
<div class="box-container">
	<div id="story" class="box">
		<% @lines.each do |line| %>
			<span class="story-line" data-id="<%=line.id%>"><%= link_to line.text, '#', :class=>"story-line" %></span>
		<% end %>

	</div>

	<div id="next-steps">
		<%= render 'next_steps' %>
	</div>


	<%= render 'invite' %>
</div>
